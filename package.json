{
  "name": "ci-projet-front",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "docker": "npm run docker:build && npm run docker:clean && npm run docker:start",
    "docker:build": "docker build -t portfolio-tech-front .",
    "docker:clean": "npm run docker:clean:container && npm run docker:clean:volume && npm run docker:clean:image",
    "docker:clean:container": "docker container prune -f",
    "docker:clean:image": "docker image prune -f",
    "docker:clean:volume": "docker volume prune -f",
    "docker:start": "docker run -d -e PORT=4200 -p 8765:4200 --name portfolio-tech-front portfolio-tech-front",
    "start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
